<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="harshwardhan.list">
      <field name="name">Harshwardhan list</field>
      <field name="model">harshwardhan.harshwardhan</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="Email"/>
          <field name="Price"/>
          <field name="PhoneNo"/>
        </tree>
      </field>
    </record>

    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="harshwardhan.action_window">
      <field name="name">Harshwardhan window</field>
      <field name="res_model">harshwardhan.harshwardhan</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->

    <record model="ir.actions.server" id="harshwardhan.action_server">
      <field name="name">Harshwardhan server</field>
      <field name="model_id" ref="model_harshwardhan_harshwardhan"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>


    <!-- Top menu item -->

    <menuitem name="Harshwardhan" id="harshwardhan.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Menu 1" id="harshwardhan.menu_1" parent="harshwardhan.menu_root"/>
    <menuitem name="Menu 2" id="harshwardhan.menu_2" parent="harshwardhan.menu_root"/>
-->
    <!-- actions -->

    <menuitem name="List" id="harshwardhan.menu_1_list" parent="harshwardhan.menu_1"
              action="harshwardhan.action_window"/>
    <menuitem name="Server to list" id="harshwardhan" parent="harshwardhan.menu_2"
              action="harshwardhan.action_server"/>

  </data>
</odoo>
