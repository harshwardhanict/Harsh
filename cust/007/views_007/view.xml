<odoo>
    <data>


        <record model="ir.ui.view" id="007_list">
            <field name="name">007 list</field>
            <field name="model">station.info</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="Price"/>
                    <field name="Ticket"/>
                    <field name="Station"/>
                    <field name="States"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="007_form">
            <field name="name">007 form</field>
            <field name="model">station.info</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name" Placeholder='Name'/>
                        <field name="Price" Placeholder='In RS/-'/>
                        <field name="Ticket" widget="Ticket"/>
                    </group>
                    <group>
                        <field name="Station"/>
                        <field name="States" widget="many2many_tags"/>
                    </group>
                </form>
            </field>
        </record>

