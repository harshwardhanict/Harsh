<odoo>
    <data>
        <!-- explicit list view definition -->

        <record model="ir.ui.view" id="rental.list">
            <field name="name">rental list</field>
            <field name="model">rental.rental</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="customer"/>
                    <field name="rental_type"/>
                    <field name="start_date"/>
                    <field name="end_date"/>
                    <field name="rental_product"/>
                    <field name="price"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>


        <!-- actions opening views on models -->

        <record model="ir.actions.act_window" id="rental_action_window">
            <field name="name">rental window</field>
            <field name="res_model">rental.rental</field>
            <field name="view_mode">tree,form</field>
        </record>


        <!-- server action to the one above -->

        <record model="ir.actions.server" id="rental.action_server">
            <field name="name">rental server</field>
            <field name="model_id" ref="model_rental_rental"/>
            <field name="state">code</field>
            <field name="code">
                action = {
                "type": "ir.actions.act_window",
                "view_mode": "tree,form",
                "res_model": model._name,
                }
            </field>
        </record>


        <!-- Top menu item -->

        <menuitem name="rental" id="rental_menu_root"/>

        <!-- menu categories -->

        <menuitem name="Menu 1" id="rental_menu_1" parent="rental_menu_root"/>
        <menuitem name="Menu 2" id="rental_menu_2" parent="rental_menu_root"/>

        <!-- actions -->

        <menuitem name="Rental Management" id="rental.menu_1_list" parent="rental_menu_root"
                  action="rental_type_action"/>
        <menuitem name="Server to list" id="rental" parent="rental_menu_2"
                  action="rental.action_server"/>

    </data>
</odoo>